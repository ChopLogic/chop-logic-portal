{
  "name": "chop-logic-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "npm run format:biome && npm run format:prettier",
    "format:biome": "biome format --write",
    "format:prettier": "prettier --write \"**/*.{css,scss,mdx,md}\"",
    "lint": "npm run lint:biome && npm run lint:prettier",
    "lint:biome": "biome check",
    "lint:prettier": "prettier --check \"**/*.{css,scss,mdx,md}\"",
    "lint:errors": "biome check --diagnostic-level error --max-diagnostics 100",
    "lint:warnings": "biome check --diagnostic-level warn --max-diagnostics 100",
    "lint:fix": "biome check --write"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "prettier": "3.8.1",
    "sass": "^1.97.3",
    "typescript": "^5"
  }
}
